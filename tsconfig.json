{
	"compilerOptions": {
	  "target": "esnext",
	  "module": "commonjs",
	  "lib": [
		"esnext",
		"dom"
	  ],
	  "strict": true,
	  "noImplicitAny": true,
	  "esModuleInterop": true,
	  "declaration": true,
	  "outDir": "dist",
	  "sourceMap": false
	},
	"include": [
	  "src/**/*"
	],
	"exclude": [
	  "node_modules",
	  "**/*.spec.ts",
	  "dist"
	]
  }
  